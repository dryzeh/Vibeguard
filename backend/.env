NODE_ENV=development
PORT=3001
HOST=0.0.0.0

DATABASE_URL="postgresql://postgres:postgres@localhost:5432/vibeguard"
JWT_SECRET="vibeguard_secure_jwt_secret_key_2024_dev_only"
JWT_EXPIRES_IN="1h"
JWT_REFRESH_EXPIRES_IN="7d"
BCRYPT_SALT_ROUNDS=10

RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
CORS_ORIGINS="http://localhost:3000,http://localhost:19006"

LOG_LEVEL=debug
LOG_FORMAT=pretty

ENABLE_AUDIT_LOGS=true
ENABLE_RATE_LIMITING=true
ENABLE_WEBSOCKET=true
ENABLE_EMAIL_VERIFICATION=false
ENABLE_PASSWORD_RESET=false
ENABLE_METRICS=false

METRICS_PORT=9090
ENABLE_TRACING=false
TRACING_SAMPLE_RATE=0.1

REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

CACHE_TTL=3600

WS_HEARTBEAT_INTERVAL=30000
WS_PATH=/ws

ADMIN_EMAIL="admin@vibeguard.se"
ADMIN_PASSWORD="change-this-in-production"
ADMIN_NAME="System Administrator"
CORS_ORIGINS="http://localhost:3000,http://localhost:19006,http://localhost:19000"
